<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!-- Added the entity Project -->
    <changeSet id="20240207-01" author="patilrajat805">
        <createTable tableName="project">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="description" type="char">
                <constraints nullable="true" />
            </column>
            <column name="group_id" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="artifact_id" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="package_id" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="building_tool" type="varchar(255)">
                <constraints nullable="false" />
            </column>
        </createTable>
    </changeSet>

    <changeSet id="20240227" author="rajat.patil02">
        <createSequence incrementBy="1" sequenceName="seq_project_id" startValue="1"/>
    </changeSet>

</databaseChangeLog>